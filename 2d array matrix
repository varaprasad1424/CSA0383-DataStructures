#include<stdio.h>
int main()
{
int m,n,i,j,max,arr[100][100],a;
printf("enter the no. of matrix:");
scanf("%d%d",&m,&n);
printf("enter the rows and columns:\n");
for (i=0;i<m;i++)
{
for (j=0;j<n;j++)
{
scanf("%d",&arr[i][j]);
}
}
max=arr[0][0];
for (i=0;i<m;i++)
{
for (j=0;j<n;j++)
{
if (arr[i][j]>max)
{
max=arr[i][j];
}
}
}
printf("max number %d",max);
getch();
clrscr();
return 0;
}
