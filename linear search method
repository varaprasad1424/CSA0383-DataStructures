#include<stdio.h>
int main()
{
int arr[100],search,c,number;
printf("enter the number of elements in array\n");
scanf("%d",&number);
printf("%d ",number);
for(c=0;c<number;c++)
scanf("%d",&arr[c]);
printf("enter the number to search\n");
scanf("%d",&search);
for(c=0;c<number;c++)
{
if (arr[c]==search )
{
printf("%d is present at location %d/n",search,c+1);
break;
}
}
if(c==number)
{
printf("%d is not present in arr \n",search);
}
return 0;
}
